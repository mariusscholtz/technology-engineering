<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from ExtendAccessWSv1Service WSDL service -->

  <service name="ExtendAccessWSv1" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://samplews.oracle.com"/>
      <parameter name="wsdlServiceElement" value="ExtendAccessWSv1Service"/>
      <parameter name="schemaQualified" value="http://samplews.oracle.com"/>
      <parameter name="wsdlServicePort" value="ExtendAccessWSv1"/>
      <parameter name="className" value="com.oracle.samplews.ExtendAccessWSv1SoapBindingImpl"/>
      <parameter name="wsdlPortType" value="ExtendAccessWSv1"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="extendAccess" qname="operNS:extendAccess" xmlns:operNS="http://samplews.oracle.com" returnQName="retNS:extendAccessReturn" xmlns:retNS="http://samplews.oracle.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:userId" xmlns:pns="http://samplews.oracle.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:eaUUID" xmlns:pns="http://samplews.oracle.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="extendAccess"/>

  </service>
</deployment>
